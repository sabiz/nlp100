    "def cipher(text):\n",
    "    return \"\".join(chr(219 - ord(c)) if c.islower() else c for c in text)\n",
    "text = \"Hello World\"\n",
    "crypted = cipher(text)\n",
    "print(crypted)\n",
    "decrypted = cipher(crypted)\n",
    "print(decrypted)\n"
